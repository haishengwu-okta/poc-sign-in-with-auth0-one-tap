<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>SignIn with AtkoID</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
    </head>
    <body>
        <script>
         const user = {
             picture: '<%= user.picture%>',
             name: '<%= user.name%>',
             email: '<%= user.email%>',
         };
         const origin = '<%= origin %>';

         window.parent && window.parent.postMessage(user, origin);
        </script>
    </body>
</html>
